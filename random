def inverseRow(L):
    oldNumRows = len(L)
    oldNumCols = len(L[0])
    newNumRows = oldNumCols
    newNumCols = oldNumRows
    newPiece = []
    for r in range(newNumRows):
        newPiece += [[None] * newNumCols]
    for r in range(oldNumRows):
        for c in range(oldNumCols):
            newPiece[c][r] = L[r][2-c]
    return newPiece
L = [[2,5],[1,4],[0,3]]
print(inverseRow(L))

